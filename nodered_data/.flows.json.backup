    {
        "id": "e5d8331f25939ab4",
        "type": "ui_template",
        "z": "29a2cc47a8487461",
        "group": "b158160e5462dfdd",
        "name": "",
        "order": 5,
        "width": 0,
        "height": 0,
        "format": "<div\n  style=\"width:100%;height:100%;background:#0b1721;border-radius:12px;overflow:hidden;border:1px solid #123;position:relative\">\n  <svg id=\"joy\" viewBox=\"-1 -1 2 2\" preserveAspectRatio=\"xMidYMid meet\" style=\"width:100%;height:100%;display:block\">\n    <!-- grid -->\n    <defs>\n      <pattern id=\"grid\" width=\"0.2\" height=\"0.2\" patternUnits=\"userSpaceOnUse\">\n        <path d=\"M 0 0 L 0 0.2 M 0 0 L 0.2 0\" stroke=\"#1e2a36\" stroke-width=\"0.003\" />\n      </pattern>\n    </defs>\n    <rect x=\"-1\" y=\"-1\" width=\"2\" height=\"2\" fill=\"url(#grid)\" />\n    <!-- axes -->\n    <line x1=\"-1\" y1=\"0\" x2=\"1\" y2=\"0\" stroke=\"#335\" stroke-width=\"0.01\" />\n    <line x1=\"0\" y1=\"-1\" x2=\"0\" y2=\"1\" stroke=\"#335\" stroke-width=\"0.01\" />\n    <!-- labels -->\n    <text x=\"0.95\" y=\"-0.05\" fill=\"#889\" font-size=\"0.12\" text-anchor=\"end\">X</text>\n    <text x=\"0.05\" y=\"-0.95\" fill=\"#889\" font-size=\"0.12\">Y</text>\n    <!-- dot -->\n    <circle id=\"dot\" cx=\"0\" cy=\"0\" r=\"0.05\" fill=\"#1ec8ff\" stroke=\"#0ff\" stroke-width=\"0.005\" />\n  </svg>\n\n  <!-- live waarden rechtsboven -->\n  <div id=\"vals\" style=\"position:absolute;right:8px;top:6px;color:#a8c7ff;font:600 12px/1.2 sans-serif;\"></div>\n</div>\n\n<script>\n  (function(scope){\n  const dot = () => document.getElementById('dot');\n  const vals = () => document.getElementById('vals');\n\n  function clamp(v,min,max){ return Math.max(min, Math.min(max, v)); }\n\n  function render(x, y){\n    // SVG-coords: X normaal, Y omhoog is + (dus we draaien om)\n    const cx = clamp(x, -1, 1);\n    const cy = -clamp(y, -1, 1);\n    const d = dot(); if(!d) return;\n    d.setAttribute('cx', cx);\n    d.setAttribute('cy', cy);\n    const v = vals(); if(v) v.textContent = `x=${x.toFixed(2)}  y=${y.toFixed(2)}`;\n  }\n\n  scope.$watch('msg', function(msg){\n    if(!msg || !msg.payload) return;\n    const p = msg.payload;\n    if (typeof p.x === 'number' && typeof p.y === 'number') {\n      render(p.x, p.y);\n    }\n  });\n})(scope);\n</script>",
