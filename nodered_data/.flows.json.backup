    {
        "id": "5a16bac9ebe4e607",
        "type": "function",
        "z": "29a2cc47a8487461",
        "name": "to_json",
        "func": "// Parse MQTT payload â†’ JSON\nif (Buffer.isBuffer(msg.payload)) {\n    msg.payload = msg.payload.toString();\n}\n\nif (typeof msg.payload === \"string\") {\n    try {\n        msg.payload = JSON.parse(msg.payload);\n    } catch (e) {\n        node.warn(\"payload is geen geldige JSON: \" + msg.payload);\n        return null; // drop onleesbare berichten\n    }\n}\n\n// laat het bericht door\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
