    {
        "id": "e5d8331f25939ab4",
        "type": "ui_template",
        "z": "29a2cc47a8487461",
        "group": "b158160e5462dfdd",
        "name": "",
        "order": 5,
        "width": 0,
        "height": 0,
        "format": "<div id=\"pad\" style=\"position:relative;width:100%;padding-top:100%;\n  background:#0b1721;border-radius:12px;overflow:hidden;border:1px solid #123\">\n    <!-- assen -->\n    <div id=\"axisX\" style=\"position:absolute;left:0;right:0;top:50%;height:1px;background:#335;\"></div>\n    <div id=\"axisY\" style=\"position:absolute;top:0;bottom:0;left:50%;width:1px;background:#335;\"></div>\n    <!-- stip -->\n    <div id=\"dot\" style=\"position:absolute;width:14px;height:14px;border-radius:50%;\n    background:#1ec8ff;box-shadow:0 0 10px #1ec8ff;\"></div>\n</div>\n\n<script>\n    (function(scope){\n  // bewaar trail (optioneel: lijntje met laatste posities)\n  let trail = [];\n  const TRAIL_LEN = 25;\n\n  function render(x, y){\n    const pad = document.getElementById(\"pad\");\n    const dot = document.getElementById(\"dot\");\n    if(!pad || !dot) return;\n\n    const w = pad.clientWidth, h = pad.clientHeight;\n    const r = 14; // diameter van de stip\n\n    // x,y ∈ [-1,1] -> pixelpositie (Y inverted voor scherm)\n    const px = ((x + 1) / 2) * (w - r);\n    const py = ((1 - (y + 1) / 2)) * (h - r);\n\n    dot.style.left = px + \"px\";\n    dot.style.top  = py + \"px\";\n\n    // optionele trail tekenen met kleine “ghost” puntjes\n    trail.push({px, py});\n    if(trail.length > TRAIL_LEN) trail.shift();\n\n    // verwijder oude ghosts\n    Array.from(pad.querySelectorAll(\".ghost\")).forEach(n => n.remove());\n    trail.forEach((p,i)=>{\n      const g = document.createElement(\"div\");\n      const s = 10 * (i / TRAIL_LEN) + 2; // kleiner naarmate ouder\n      g.className = \"ghost\";\n      g.style.cssText = `position:absolute;width:${s}px;height:${s}px;border-radius:50%;\n        background:#1ec8ff55;left:${p.px + (r-s)/2}px;top:${p.py + (r-s)/2}px`;\n      pad.appendChild(g);\n    });\n  }\n\n  scope.$watch('msg', function(msg){\n    if(!msg || !msg.payload) return;\n    const p = msg.payload;\n    if(typeof p.x === \"number\" && typeof p.y === \"number\"){\n      render(p.x, p.y);\n    }\n  });\n})(scope);\n</script>",
